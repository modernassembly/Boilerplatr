{
    "output_file_dir":  "./",
    "file_filter":      ["!.DS_Store", "!thumbs.db"],
    "dir_filter":       ["!.svn", "!.git", "!.sass-cache"],
    "file_mapping":     [
        {
            "input":    "/js/example.jquery.js",
            "output":   "/js/<%= it.pluginName %>.jquery.js"
        }
    ],
    "var_mapping":      {
        "pluginName":{
            "description": "Enter the name of your jQuery plugin",
            "type":"string",
            "default": "example"
        }
    }
}